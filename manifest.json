{
  "manifest_version": 2,
  "name": "History Search",
  "version": "1.0.0",
  "description": "Search browsing history",
  "author": "d0iasm",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48_2.png",
    "128": "icons/icon128_2.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_title": "History Search",
    "default_popup": "popup.html"
  },
  "permissions": [
    "history"
  ]
}
